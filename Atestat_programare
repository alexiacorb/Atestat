#include <iostream>
#include <fstream>
ifstream fin("atestat.in");
ofstream fout("atestat.out");
using namespace std;
int n,x[31];
void citire(){
    fin>>n;
    for(int i=1;i<=n;i++){
        fin>>x[i];
    }
}
int prin(int n){
    if(n==1 || n==0){
        return 0;
    }
    for(int d=2;d*d<=n;d++){
        if(n%d==0){
            return 0;
        }
    }
    return 1;
}
int main()
{
    int minn=0,maxx=0;
    citire();
    for(int i=1;i<=n;i++){
        if(prim(x[i])==1){
            cout<<x[i]<<" ";
        }
    }
    cout<<endl;
    for(int i=1;i<=n;i++){
        if(v[i]<minn && v[i]%2==1){
            minn=x[i];
        }
        if(v[i]>maxx && v[i]%2==0){
            maxx=x[i];
        }
    }
    cout<<minn<<" "<<maxx<<endl;
    for(int i=1;i<=n-1;i++){
        for(int j=i+1;j<=n;j++){
            if(prim(x[i])==1 && prim(x[j])==1 && x[i]+x[j]==maxx && x[i]%2==1 && x[j]%2==1){
                cout<<x[i]<<" "<<x[j];
                break;
            }
        }
    }
    
    return 0;
}
