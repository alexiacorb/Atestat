Subiectul 2
#include <iostream>
#include <fstream>
ifstream fin("atestat.in");
ofstream fout("atestat.out");
using namespace std;
int n,x[31];
void citire(){
    fin>>n;
    for(int i=1;i<=n;i++){
        fin>>x[i];
    }
}
int prin(int n){
    if(n==1 || n==0){
        return 0;
    }
    for(int d=2;d*d<=n;d++){
        if(n%d==0){
            return 0;
        }
    }
    return 1;
}
int main()
{
    int minn=0,maxx=0;
    citire();
    for(int i=1;i<=n;i++){
        if(prim(x[i])==1){
            cout<<x[i]<<" ";
        }
    }
    cout<<endl;
    for(int i=1;i<=n;i++){
        if(v[i]<minn && v[i]%2==1){
            minn=x[i];
        }
        if(v[i]>maxx && v[i]%2==0){
            maxx=x[i];
        }
    }
    cout<<minn<<" "<<maxx<<endl;
    for(int i=1;i<=n-1;i++){
        for(int j=i+1;j<=n;j++){
            if(prim(x[i])==1 && prim(x[j])==1 && x[i]+x[j]==maxx && x[i]%2==1 && x[j]%2==1){
                fout<<x[i]<<" "<<x[j];
                break;
            }
        }
    }
    
    return 0;
}
PROBLEMA 3
#include <iostream>
#include <fstream>
using namespace std;
ifstream fin("atestat.in");
ofstream fout("atestat.out");
int n,x[100];
void citire(){
    fin>>n;
    for(int i=1;i<=n;i++){
        fin>>x[i];
    }
}
int main()
{
    citire();
    for(int i=1;i<=n;i++)
    {
        if(x[i]%2==0){
            cout<<x[i]<<" ";
        }
    }
    cout<<endl;
    int b=0,a=100000000;
    for(int i=1;i<=n;i++)
    {
        if(x[i]>b){
            b=x[i];
        }
        if(x[i]<a){
            a=x[i];
        }
    }
    cout<<a<<endl;
    cout<<b<<endl;
    int r=0;
    while(b!=0){
        r=a%b;
        a=b;
        b=r;
    }
    fout<<a;
    return 0;
}
